pipeline{
  agent any
    stages('Build'){
      steps{
        sh 'g++ error.cpp -o error'
        build job: 'PES2UG20CS035-1', wait: false
        echo 'Build stage successful'
      }
     }
     stage('Test'){
      steps{
        sh 'cat pes2ug20cs035.cpp'
        echo 'test stage successful'
      }
     }
     stage('Deploy'){
      steps{
     
      echo 'Deployment stage successful'
      }
     }
     post{
      failure{
      echo 'pipeline failed'
      }
     }
}
   
